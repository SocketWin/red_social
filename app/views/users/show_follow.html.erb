<% provide(:title, @user.login + @title) %>
<div class="row">
  <aside class="span4">
    <section>
      <%= render 'shared/user_info', object: @user %>
    </section>
  </aside>
  <div class="span8">

    <h3><%= @user.login + @title %></h3>
    <% if @users.any? %>
        <ul class="users">
          <%= render @users %>
        </ul>
        <%= will_paginate %>
    <% end %>
  </div>
</div>